<% if @report.nil? %>
  <h3>Report not found.</h3>
<% else  %>
  <fieldset>
    <p class="lead"><%= @title %></p>
    <%= render :partial => "layouts/flash_msg" %>
    <%= render :partial => "shared/report_chart_and_html", :locals => { :html => @html, :chart => @zgraph, :report => @report } %>
  </fieldset>
<% end  %>
