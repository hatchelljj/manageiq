<div id="bottlenecks_summary_div">
  <% if x_node != "" %>
    <%= render(:partial => "layouts/flash_msg", 
               :locals  => {:div_num => "1"})
    %>
    <%= render(:partial => 'bottlenecks_options', 
               :locals  => {:typ => "summ"})
    %>
  <% else %>
    <div class="bg-warning">
      <span class="glyphicon glyphicon-info-sign"></span>
      <strong>Select a node on the left to view Bottlenecks timeline.</strong>
    </div>
  <% end %>
  <%= render :partial=>'bottlenecks_tl_detail' %>
</div>
